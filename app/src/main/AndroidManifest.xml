<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.drn.diarioderegistro">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.camera2"
        android:required="true" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- android:label="@string/app_name" -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Aplicacion de prueba"
        android:supportsRtl="true"
        android:theme="@style/modificadoTest">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Tomar_Foto" />
        <activity android:name=".VerCalendario" />
        <activity android:name=".RegistrosDiarios" />
        <activity android:name=".show_upload" />
        <activity android:name=".Donde"></activity>
    </application>

</manifest>